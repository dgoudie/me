{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "backtick"
        ],
        "no-console": [
            false
        ],
        "object-literal-sort-keys": false,
        "variable-name": {
            "options": [
                "allow-leading-underscore"
            ]
        },
        "interface-name": false,
        "jsx-self-close": true,
        "jsx-no-multiline-js": false
    },
    "rulesDirectory": []
}